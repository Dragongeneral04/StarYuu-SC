
BGForDirt = 888
BGForCaveBackground = 880
BGForLava = 880
BGForRock = 880

GenerateRandomWorld = 1
startdf = true
dfcount = 0


function webhuk(Logs)
    local script = [[
        $webHookUrl = "]].. Webhook ..[["
        $cpu = (Get-WmiObject win32_processor | Measure-Object -property LoadPercentage -Average | Select Average).Average
        [System.Collections.ArrayList]$embedArray = @()
    $thumbnailObject = [PSCustomObject]@{
        url = "https://cdn.discordapp.com/attachments/1031531937904467969/1031532036445454437/1213984.jpg"
    }
    $color = Get-Random -Minimum 0 -Maximum 16777215
    $title = 'Staryuu Auto DirtFarm'
    $description = "]].. Logs..[["

    $footer = [PSCustomObject]@{
        icon_url = "https://cdn.discordapp.com/attachments/1031531937904467969/1031532036445454437/1213984.jpg"
        text = "]].."Staryuu | Date : "..(os.date"%d/%m/%y":upper().." Time : ")..os.date("%I")..":"..os.date("%M").." "..os.date("%p"):upper()..[["
    }


    $embedObject = [PSCustomObject]@{
        color = $color
        title = $title
        description = $description
        thumbnail = $thumbnailObject
        footer = $footer
    }

    [System.Collections.ArrayList]$embedArray = @()
    $embedArray.Add($embedObject)

    $payload = [PSCustomObject]@{
        embeds = $embedArray
    }
    [Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12
    Invoke-RestMethod -Uri $webHookUrl -Body ($payload | ConvertTo-Json -Depth 4) -Method Post -ContentType 'application/json'
    ]]

    local pipe = io.popen("powershell -command -", "w")
    pipe:write(script)
    pipe:close()
  end

  function LogTheName(text)
    file = io.open("WorldLogs.txt", "a")
    file:write(text.."\n")
    file:close()
   end

  function ToinkBG()
    count = 0
    for _, tile in pairs(getTiles()) do
        if tile.bg ~= 0 then
            count = count + 1
        end
    end
    return count
end

function ToinkFG()
    count = 0
    for _, tile in pairs(getTiles()) do
        if tile.fg ~= 0 then
            count = count + 1
        end
    end
    return count
end

function GenerateAWorld()
    ::BACK31::
    sleep(3000)
    sendPacket(3,"action|quit_to_exit")
    sleep(3000)
    local WorldsToGenerate = ""
    for i = 1,Main.WorldLetters do
        WorldsToGenerate = WorldsToGenerate..string.char(math.random(97,122))
    end
    sendPacket(3,"action|join_request\nname|" .. WorldsToGenerate.."\ninvitedWorld|0")
    sleep(5000)

    if ToinkBG() > 3600 or ToinkFG() > 3601 then
        goto BACK31
    end

    for _,t in pairs(getTiles()) do
        if t.fg== 242 or t.fg== 9640 or t.fg== 202 or t.fg== 204 or t.fg== 206 or t.fg== 1796 or t.fg== 4994 or t.fg== 7188 or t.fg== 2408 or t.fg== 2950 or t.fg== 4428 or t.fg== 4802 or t.fg== 5814 or t.fg== 5260 or t.fg== 5980 or t.fg== 8470 or t.fg== 10410 or t.fg== 11550 or t.fg== 11586 then
            goto BACK31
        end
    end
end


function PutWlAndJammer()
    if Main.useWL == true then
        place(242,0,-1)
        sleep(800)
        place(226,1,-1)
        sleep(800)
        punch(1,-1)
        sleep(800)
        place(SubSetting.enteranceID,-1,0)
        sleep(800)
        place(SubSetting.enteranceID,1,0)
        sleep(800)
    else
        hit = 8
        pl = 2
       
        function reapplyLock(x,y)
            wrench(x,y)
            sleep(2000)
            sendPacket(2,"action|dialog_return\ndialog_name|lock_edit\ntilex|"..(math.floor(getBot().x/32)+x).."|\ntiley|"..(math.floor(getBot().y/32)+y).."|\nbuttonClicked|recalcLock\n\ncheckbox_public|0\ncheckbox_ignore|1")
        end
        function editDoor(label,world,doorid,x,y)
            wrench(x,y)
            sleep(1000)
            sendPacket(2,"action|dialog_return\ndialog_name|door_edit\ntilex|"..math.floor(getBot().x/32)+x.."|\ntiley|"..math.floor(getBot().y/32)+y.."|\ndoor_name|"..label.."\ndoor_target|"..world.."\ndoor_id|"..doorid.."\ncheckbox_locked|1")
            say("done")
        end
        
     

        for i=1 ,hit do
            punch(-1,1)
            sleep(300)
            punch(-2,1)
            sleep(300)
            punch(-2,2)
            sleep(300)
            punch(-1,2)
            sleep(300)
            punch(0,2)
            sleep(300)
            punch(1,2)
            sleep(300)
            punch(2,2)
            sleep(300)
            punch(2,1)
            sleep(300)
            end
            place(202,0,-1)
            sleep(800)
            place(2,-1,-1)
            sleep(800)
            place(12,-1,1)
            sleep(800)
            place(226,1,-1)
            sleep(800)
            punch(1,-1)
            sleep(800)
            place(SubSetting.enteranceID,-1,0)
            sleep(800)
            place(SubSetting.enteranceID,1,0)
            sleep(800)
            reapplyLock(0,-1)
            sleep(800)
            editDoor(Doors.Label, Doors.Destination, Doors.ID,-1,1)
    
            for i=1 ,pl do
            place(2,-2,1)
            sleep(300)
            place(2,2,1)
            sleep(300)
            end
        MoveWhenInWhiteDoor()
        end  
end

function SeedsScanner(ID)
    Count = 0
    for _, obj in pairs(getObjects()) do 
        if obj.id == ID then 
            Count = Count + obj.count
        end
    end
    return Count
end

function DropTheItems(itemID, count)
    if findItem(itemID) >= count then
        sendPacket(2, "action|drop\nitemID|" .. itemID)
        sleep(500)
        sendPacket(2,"action|dialog_return\ndialog_name|drop_item\nitemID|" .. itemID .. "|\ncount|" .. count)
        sleep(1000)
    end
end

function TrashTheJunks()
    for i, trash in ipairs(SubSetting.TrashList) do
        if findItem(trash) > 0 then
    sendPacket(2, "action|trash\n|itemID|" .. trash)
    sleep(2000)
    sendPacket(2, "action|dialog_return\ndialog_name|trash_item\nitemID|" .. trash .. "|\ncount|" .. findItem(trash))
    sleep(2000)
        end
    end
    TrashJunks = {2, 14}
    for i, trash in ipairs(TrashJunks) do
        if findItem(trash) > 195 then
            sendPacket(2, "action|trash\n|itemID|" .. trash)
            sleep(2000)
            sendPacket(2, "action|dialog_return\ndialog_name|trash_item\nitemID|" .. trash .. "|\ncount|" .. 10)
            sleep(2000)
        end
    end
end

function Reconnect()
    if getBot().status ~= "online" then
        webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Bot Is Offline Trying To Reconnect.... \n<:red_circle:987661002868936774> | Status : Offline")
        while getBot().status ~= "online" do
            connect()
            sleep(15000)
        end
        webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Bot Is Back Online\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
    end
end

function MoveWhenInWhiteDoor()
    while getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).fg == 6 do
        move(3,0)
        sleep(500)
        move(1,0)
        sleep(500)
        if getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).fg == 0 then 
            break
        end
    end
end



function ClearTheSide()


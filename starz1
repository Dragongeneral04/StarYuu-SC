function TakeWL()
    local CurrentWorld = getBot().world
    if findItem(242) < 1 then
        sendPacket(3,"action|join_request\nname|" ..worlds.StoragePlatform.."\ninvitedWorld|0")
        sleep(5000)
        sendPacket(3,"action|join_request\nname|" ..worlds.StoragePlatform.. "|" ..worlds.StoragePlatformID.."\ninvitedWorld|0")
        sleep(4500)
        collect(2)
        sleep(4500)
        sendPacket(3,"action|join_request\nname|" ..CurrentWorld.."\ninvitedWorld|0")
        sleep(5000)
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(200)
    end
end
function TakeAndPlacePlatform()
    webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Bot Is Going To Take Platform In "..StoragePlatform.." Then Placing It In "..getBot().world.."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
    local CurrentWorld = getBot().world
    if findItem(SubSetting.Platfoms_ID) < 52 then
        sendPacket(3,"action|join_request\nname|" ..worlds.StoragePlatform.."\ninvitedWorld|0")
        sleep(5000)
        sendPacket(3,"action|join_request\nname|" ..worlds.StoragePlatform.. "|" ..worlds.StoragePlatformID.."\ninvitedWorld|0")
        sleep(4500)
        collect(2)
        sleep(4500)
        if findItem(SubSetting.Platfoms_ID) > 104 then
            move(-1,0)
            DropTheItems(SubSetting.Platfoms_ID,findItem(SubSetting.Platfoms_ID) - 104)
        end
        sendPacket(3,"action|join_request\nname|" ..CurrentWorld.."\ninvitedWorld|0")
        sleep(5000)
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(200)
    end
    for i = 2, 52, 2 do
        while getTile(1,i).fg == 0 do
            if findItem(SubSetting.Platfoms_ID) == 0 then
                break
            end
            findPath(1,i-1)
            place(SubSetting.Platfoms_ID,0,1)
            sleep(190)
        end
    end
    for i = 2, 52, 2 do
        while getTile(98,i).fg == 0 do
            if findItem(SubSetting.Platfoms_ID) == 0 then
                break
            end
            findPath(98,i-1)
            place(SubSetting.Platfoms_ID,0,1)
            sleep(190)
        end
    end
end


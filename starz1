function GetDirtSeeds()
    local CurrentWorld = getBot().world
    if findItem(3) == 0 then
        sleep(1500)
        sendPacket(3,"action|join_request\nname|".. worlds.Save_Seed_World.."\ninvitedWorld|0")
        sleep(5000)
        sendPacket(3,"action|join_request\nname|".. worlds.Save_Seed_World .. "|" .. worlds.Save_Seed_DoorID.."\ninvitedWorld|0")
        sleep(4500)
        for _, floating in pairs(getObjects()) do 
            if floating.id == 3 then 
                findPath((floating.x+10) / 32, (floating.y+10) / 32)
                sleep(800)
                collect(2)
                sleep(1500)
            end
        end
        if findItem(3) > 1 then
            webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Dirt Seeds Is More Than 1... Going Back On Creating Dirt Farms For You My Master.\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. CurrentWorld.."\ninvitedWorld|0")
            sleep(5000)
        end
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(500)
    end
end

function PlantAndHarvestDirt()
    while findItem(2) == 0 do
        for x = 2, 22  do
            if findItem(2) == 200 then 
                break
            end
            if findItem(3) == 0 then
                webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Dirt Seeds Is 0... Taking Dirt Seeds In "..worlds.Save_Seed_World .. "\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
                GetDirtSeeds()
                sleep(250)
                webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Going To Place Dirts To Make A Layer \n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
            end
            while getTile(x, 21).ready do
                findPath(x, 21)
                punch(0, 0)
                sleep(155)
                collect(2)
            end
            while getTile(x, 21).fg == 0 and findItem(3) > 0 do
                findPath(x, 21)
                place(3, 0, 0)
                sleep(90)
            end
            Reconnect()
        end
    end
end

function PlaceTheDirts()
    webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information :  Going To Place Dirts To Make A Layer  \n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
    for y = 52, 2, -2 do
        for x = 2, 97 do
            if getTile(x, y).fg == 0 then
                PlantAndHarvestDirt()
                findPath(x, y - 1)
                while getTile(x, y).fg == 0 and findItem(2) > 0 do
                    place(2, 0, 1)
                    sleep(190)
                end
                Reconnect()
            end
        end
    end
end


function DirtFarmDone()
    dfcount = dfcount + 1
    for x = 2, 22  do
        while getTile(x,21).fg == 3 do
            findPath(x,21)
            punch(0,0)
            sleep(190)
            TrashTheJunks()
            if getTile(x,21).fg == 0 then
                collect(2)
                sleep(40)
            end 
            Reconnect()
        end
    end
end

function BreakTheSignalJammer()
    if Main.BreakJammer == true then
        for _,t in pairs(getTiles()) do
            if t.fg == 226 then
              findPath(t.x, t.y-1)
              sleep(250)
              while getTile(t.x, t.y).fg ~= 0 or getTile(t.x, t.y).bg ~= 0 do
                punch(0,1)
                sleep(125)
              end
              sleep(250)
              place(2,0,1)
            end
        end
    end
    if dfcount == Main.DirtFarm then
        startdf = false
    else
        Main.Auto_Find_world = true
    end
    
end

function NeedsToGenerateAWorld()
    if GenerateRandomWorld == 1 then
        webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Finding "..Main.WorldLetters.." Letter World Please Wait.\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."") 
        GenerateAWorld()
        sleep(300)
        PutWlAndJammer()
    end

    if GenerateRandomWorld == 1 then
        LogTheName("World : "..getBot().world) 
        sleep(300)
        webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : "..Main.WorldLetters.." Letter World Found \n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
        sleep(500)
    end
end


while startdf do
    if findItem(242) < 1 and Main.useWL == true then
        TakeWL()
    end
    if Main.Auto_Find_world == true then
        NeedsToGenerateAWorld()
    end
    MoveWhenInWhiteDoor()
    ClearTheSide()
    MoveWhenInWhiteDoor()
    TakeAndPlacePlatform()
    MoveWhenInWhiteDoor()
    ClearingDirts()
    if Main.BreakTheRock == true then
    ClearTheRocks()
    end
ClearTheLavas()
PlaceTheDirts()

    end
end

function SaveTheDirtSeeds()
    if SaveSettings.SaveDirt == true then 
        local CurrentWorld = getBot().world 
        if findItem(3) > 150 then 
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. worlds.Save_Seed_World.."\ninvitedWorld|0")
            sleep(5000)
            sendPacket(3,"action|join_request\nname|".. worlds.Save_Seed_World .. "|" .. worlds.Save_Seed_DoorID.."\ninvitedWorld|0")
            sleep(4500)
        end
        for _, tile in pairs(getTiles()) do 
            if tile.fg == BGForDirt or tile.bg == BGForDirt then 
                sleep(500)
                findPath(tile.x, tile.y)
                sleep(800)
            end
        end
        while findItem(3) > 100 do 
            drop(3)
            sleep(1500)
            move(-1,0)
            sleep(500)
        end
        if findItem(3) < 150 then
            webhuk("<:Bot:1035169530143576145> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Done Dropping Dirt Seeds... Total Dirt Seeds Dropped ".. SeedsScanner(3) .."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."\n===========================\n**Bot Backpack**\n<:ladder:1031872064400916530> | Platforms Remain : "..findItem(SubSetting.Platfoms_ID).."\n<:wl:1034385539853463563> | WorldLock Remain : "..findItem(242).."")
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. CurrentWorld.."\ninvitedWorld|0")
            sleep(5000)
        end
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(500)
    else
        sleep(2000)
        sendPacket(2, "action|trash\n|itemID|" .. 3)
        sleep(2000)
        sendPacket(2, "action|dialog_return\ndialog_name|trash_item\nitemID|" .. 3 .. "|\ncount|" .. findItem(3))
        sleep(2000)
    end
end

